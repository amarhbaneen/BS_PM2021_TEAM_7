pipeline {
    agent none
    stages {

        stage('Test') {
            agent {
                docker {
                    image 'amarhabneen/bm_team7'
                }
            }
            steps {
                sh 'python manage.py test'
            }

        }
         stage('run app') {
            agent {
                docker {
                    image 'amarhabneen/bm_team7'
                }
            }
            steps {
                sh 'python manage.py runserver'
            }

        }
    }
}